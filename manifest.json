{
  "manifest_version": 2,

  "name": "Chronicle",
  "description": "Chronicle add-on",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "history",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "page": "background/background.html"
  }
}